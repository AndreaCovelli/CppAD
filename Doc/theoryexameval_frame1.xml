</mtd><mtd columnalign="left" >
<msub><mi mathvariant='italic'>a</mi>
<mn>1</mn>
</msub>
<mo stretchy="false">+</mo>
<mn>2</mn>
<msub><mi mathvariant='italic'>a</mi>
<mn>2</mn>
</msub>
<mi mathvariant='italic'>x</mi>
</mtd><mtd columnalign="left" >
<mo stretchy="false">=</mo>
<mn>7</mn>
</mtd></mtr></mtable>
</mrow></math>

