# Cvs does not maintain permissions well, so use this script to set them
chmod -x AUTHORS
chmod -x BUGS
chmod -x COPYING
chmod -x ChangeLog
chmod -x CoinCsro.txt
chmod -x CppAD.tex
chmod -x Developer.omh
chmod -x Doc.omh
chmod -x INSTALL
chmod -x NEWS
chmod -x README
chmod -x UWCopy040507.html
chmod -x omh/gpl.txt
chmod -x omh/*.omh
chmod -x CppAD/local/*.h
chmod -x */*.sln
chmod -x */*.vcproj
chmod -x */*.cpp
chmod -x */*.h
